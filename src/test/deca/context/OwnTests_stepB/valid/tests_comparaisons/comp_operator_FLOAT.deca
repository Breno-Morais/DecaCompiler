//Test de tous les opérateurs de comparaison (doit renvoyer un booléen) avec des FLOAT
//

{
    //inférieur ou égal
    boolean b = 1.0 <= 2.0;
    b = 2.0 <= 2.0;
    b = 3.0 <= 2.0;
    //inférieur strict
    b = 1.0 < 2.0;
    b = 3.0 < 2.0;
    //egalité
    b = 1.0 == 2.0;
    b = 1.0 == 1.0;
    //pas égale
    b = 1.0 != 1.0;
    b = 1.0 != 2.0;
    //supérieur ou égal
    b = 3.0 >= 2.0;
    b = 2.0 >= 2.0;
    b = 1.0 >= 2.0;
    //supérieur strict
    b = 3.0 > 2.0;
    b = 1.0 > 2.0;
}