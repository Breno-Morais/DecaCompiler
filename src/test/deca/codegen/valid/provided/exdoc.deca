// Un exemple de programme Deca

class A {
   protected int x ;
   int getX() {
      return x ;
   }
   void setX(int x) {
      this.x = x ;
   }
}

class B extends A {
   protected int y ;
   int getY() {
      return y ;
   }
   void setY(int y) {
      this.y = y ;
   }
}

{
   A a = new A() ;
   B b = new B() ;
   a.setX(2) ;
   //b.setX(a.getX() + 1);
   b.setY(a.getX() + 2);
   println("a.getX() = ", a.getX()) ;
   println("b.getX() = ", b.getX()) ;
   println("b.getY() = ", b.getY()) ;
}
